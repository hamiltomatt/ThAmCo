@model IEnumerable<ThAmCo.Events.Models.EventStaffViewModel>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StaffName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StaffName)
            </td>
            <td>
                @Html.ActionLink("Remove", "Delete", "Staffings", new { id = item.StaffId, eventId = item.EventId }) |
                @Html.ActionLink("Details", "Details", "Staff", new { id = item.StaffId })
            </td>
        </tr>
}
    </tbody>
</table>